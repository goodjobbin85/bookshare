
<div align="center">
  <h1> All Users | <a href="/users/new">Create New User</a> </h1>

  <ul>
      <% @users.each do |user| %>
        <li><a href="/users/<%= user.id %>"><%= user.name %></a> |
        <% if current_user.id == user.id %>
          <a href="/users/<%= user.id %>/edit">Edit</a></li>
        <% end %>
      <% end %>
  </ul>
</div>
